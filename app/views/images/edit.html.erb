<div class="page-header"><h1>Edit Image</h1></div>

<%= form_for @image do |f| %>


  <div class="form-group">
    <%= f.label :caption %>
    <%= f.text_field :caption, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control'%>
  </div>

  <%= f.submit "Submit", class: 'btn btn-primary' %>
<% end %>
